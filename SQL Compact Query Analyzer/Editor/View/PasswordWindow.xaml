<Window x:Class="ChristianHelle.DatabaseTools.SqlCe.QueryAnalyzer.View.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Password Protected"
        Height="145"
        Width="333">
    <Grid>
        <Label Content="Password:"
               Height="28"
               HorizontalAlignment="Left"
               Margin="21,21,0,0"
               VerticalAlignment="Top" />
        <PasswordBox Height="28"
                     Margin="91,21,23,0"
                     Name="passwordBox"
                     VerticalContentAlignment="Center"
                     VerticalAlignment="Top" />
        <Button Content="OK"
                IsDefault="True"
                Height="23"
                HorizontalAlignment="Left"
                Margin="91,63,0,0"
                Name="btnOk"
                VerticalAlignment="Top"
                Width="75"
                Click="btnOk_Click" />
        <Button Content="Cancel"
                IsCancel="True"
                Height="23"
                HorizontalAlignment="Left"
                Margin="172,63,0,0"
                Name="btnCancel"
                VerticalAlignment="Top"
                Width="75"
                Click="btnCancel_Click" />
    </Grid>
</Window>
